@startuml product inventory

     entity user {
        * id INT(11) <<PK>>
        --
        * name VARCHAR(100)
        * email VARCHAR(100)
        * password VARCHAR(256)
          phone_number
          created_at TIMESTAMP NULL
          updated_at TIMESTAMP NULL
    }

    entity product {
        * id VARCHAR(25) <<PK>> uniqid()
        --
        * name VARCHAR(100)
        * price INT(11)
        * qty INT(11)
          created_at TIMESTAMP NULL
          updated_at TIMESTAMP NULL
    }

   

@enduml